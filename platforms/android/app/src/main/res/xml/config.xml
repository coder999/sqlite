<?xml version='1.0' encoding='utf-8'?>
<widget id="com.brodybits.starter" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <name>cordova-sqlite-storage-starter-app</name>
    <description>
        Cordova sqlite storage starter app
    </description>
    <author email="brodybits@litehelpers.net" href="https://github.com/brodybits">
        @brodybits (Christopher J. Brody aka Chris Brody)
    </author>
    <content src="index.html" />
    <preference name="loglevel" value="DEBUG" />
</widget>
